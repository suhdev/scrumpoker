 } from \"./votesTable\";\n\nexport const TicketVoting: React.FC = () => {\n  const { sessionId, ticketId } = useParams() as {\n    sessionId: string;\n    ticketId: string;\n  };\n  return (\n    <ScrumPokerInner\n      key={`${sessionId}_${ticketId}`}\n      sessionId={sessionId}\n      ticketId={ticketId}\n    />\n  );\n};\n\nconst ScrumPokerInner: React.FC<{\n  sessionId: string;\n  ticketId: string;\n}> = observer(({ sessionId, ticketId }) => {\n  const { user } = useUserIdentity();\n  const [vm] = useState(() => new ScrumPokerModel());\n\n  useEffect(() => {\n    return vm.setup(sessionId, ticketId, user as IUserIdentity);\n  }, [vm, sessionId, ticketId, user]);\n\n  return (\n    <Wrapper>\n      <Vote model={vm} ticketId={ticketId} />\n    </Wrapper>\n  );\n});\n\nconst Vote: React.FC<{ model: ScrumPokerModel; ticketId: string }> = observer(\n  ({ model, ticketId }) => {\n    return (\n      <>\n        <FlexSpaceBetween direction={\"row\"}>\n          <VoteHeading>Ticket: {ticketId}</VoteHeading>\n          {model.started || !model.canStart ? null : (\n            <Button intent=\"primary\" onClick={() => model.start()}>\n              Start\n            </Button>\n          )}\n        </FlexSpaceBetween>\n        <br />\n        <div className=\"bp3-callout bp3-intent-primary bp3-icon-info-sign\">\n          <h4 className=\"bp3-heading \">Voting on estimate for {ticketId}</h4>\n          Once the vote start, please select a value from the list below. Please\n          consider complexity of the acceptance criteria compared to past\n          experience.\n        </div>\n        {(model.percentage < 1 || !model.started) && (\n          <>\n            <CenterAlignedRow style={{ marginTop: \"1rem\" }}>\n              <Spinner\n                intent=\"primary\"\n                value={model.started ? model.percentage : undefined}\n              />\n              <span style={{ marginLeft: \"1rem\" }}>\n                {model.started ? \"Voting\" : \"Waiting for voting to start\"}\n              </span>\n            </CenterAlignedRow>\n          </>\n        )}\n        {model.started && <VoteInput model={model} />}\n\n        <VotesTable model={model} />\n        {model.percentage >= 1 ? <GoToTicket model={model} /> : null}\n      </>\n    );\n  }\n);\n\nconst VoteHeading = styled.div`\n  font-size: 24px;\n`;\n\nconst VoteInput: React.FC<{ model: ScrumPokerModel }> = observer(\n  ({ model }) => {\n    const { user } = useUserIdentity();\n    const { ticketId } = useParams() as { ticketId: string };\n    return model.percentage < 1 ? (\n      <VoteWrapper>\n        <h4 className=\"bp3-heading\">\n          Enter your vote below for ticket: {ticketId}\n        </h4>\n        <ControlGroup fill={true} vertical={false}>\n          {[1, 2, 3, 5, 8, 13, 21].map((ticket) => (\n            <Observer key={ticket}>\n              {() => (\n                <VoteButton\n                  large\n                  intent={\n                    model.getVoteForUser(user!.id) === ticket\n                      ? \"primary\"\n                      : undefined\n                  }\n                  onClick={() => {\n                    model.vote(user!.id, `${ticket}`);\n                  }}\n                >\n                  {ticket}\n                </VoteButton>\n              )}\n            </Observer>\n          ))}\n        </ControlGroup>\n      </VoteWrapper>\n    ) : null;\n  }\n);\n\nconst VoteWrapper = styled.div`\n  margin-top: 1rem;\n`;\n\nconst VoteButton = styled(Button)`\n  padding-left: 20px;\n  padding-right: 20px;\n`;\n\nconst GoToTicket: React.FC<{ model: ScrumPokerModel }> = observer(\n  ({ model }) => {\n    const [value, setValue] = useState(\"\");\n    const { sessionId } = useParams() as {\n      sessionId: string;\n      ticketId: string;\n    };\n    const history = useHistory();\n    return model.percentage >= 1 ? (\n      <>\n        <br />\n        <h3>Enter ticket number to vote on next</h3>\n        <ControlGroup fill={true} vertical={false}>\n          <InputGroup\n            placeholder=\"Ticket number\"\n            value={value}\n            onChange={(e) => setValue(e.target.value)}\n          />\n          <Button\n            icon=\"chevron-right\"\n            onClick={() => {\n              if (value.trim()) {\n                history.push(\n                  `/sessions/${sessionId}/scrum-poker/${value\n                    .trim()\n                    .toLowerCase()}`\n                );\n              }\n            }}\n          >\n            Go to ticket\n          </Button>\n        </ControlGroup>\n      </>\n    ) : null;\n  }\n);\n\nconst Wrapper = styled.div`\n  padding: 32px;\n  max-width: 600px;\n  margin-left: auto;\n  margin-right: auto;\n`;\n","/home/suhdev/Code/suhdev/scrumpoker/src/login/attemptLogin.tsx",[],"/home/suhdev/Code/suhdev/scrumpoker/src/login/logout.tsx",[],"/home/suhdev/Code/suhdev/scrumpoker/src/startPage/index.tsx",[],"/home/suhdev/Code/suhdev/scrumpoker/src/layout/sessionName.tsx",[],"/home/suhdev/Code/suhdev/scrumpoker/src/layout/topNav.tsx",["134","135","136"],"/home/suhdev/Code/suhdev/scrumpoker/src/hooks/useSessionIdParam.ts",[],"/home/suhdev/Code/suhdev/scrumpoker/src/theme/index.tsx",[],"/home/suhdev/Code/suhdev/scrumpoker/src/startPage/createNewSession.tsx",[],"/home/suhdev/Code/suhdev/scrumpoker/src/startPage/joinSession.tsx",[],"/home/suhdev/Code/suhdev/scrumpoker/src/hooks/useResize.tsx",[],"/home/suhdev/Code/suhdev/scrumpoker/src/assets/logo.tsx",[],"/home/suhdev/Code/suhdev/scrumpoker/src/helpers/align.tsx",[],"/home/suhdev/Code/suhdev/scrumpoker/src/profile/index.tsx",[],{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","replacedBy":"140"},{"ruleId":"141","severity":1,"message":"142","line":1,"column":26,"nodeType":"143","messageId":"144","endLine":1,"endColumn":29},{"ruleId":"141","severity":1,"message":"145","line":4,"column":10,"nodeType":"143","messageId":"144","endLine":4,"endColumn":19},{"ruleId":"